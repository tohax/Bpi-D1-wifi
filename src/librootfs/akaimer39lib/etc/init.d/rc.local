#!/bin/sh

# Adding Hwclock to the system
echo ds1307 0x68 > /sys/class/i2c-adapter/i2c-0/new_device
hwclock --hctosys

ifconfig lo up

#modprobe 8192cu
modprobe 8188eu

# Cron
/usr/sbin/crond

# Dmesg monitor
/etc/init.d/mon.sh &

echo "5120" > /proc/sys/vm/min_free_kbytes

